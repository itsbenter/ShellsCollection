        {
            "LogName": "Overrides recipes to use Context Tags when SpaceCore is installed",
            "Action": "EditData",
            "Target": "spacechase0.SpaceCore/CookingRecipeOverrides",
            "When": {
                "FlexibleRecipe": true
            },
            "Entries": {
                "{{ModId}}_BraisedScallopRecipe": {
                    //Forage of Ferngill has Ferngill Scallop
                    //More New Fish
                    "ProductQualifiedId": "{{ModId}}_BraisedScallop",
                    "ProductAmount": 1,
                    "Ingredients": [
                        {
                            "Type": "Item",
                            "Value": "(O)88",
                            "Amount": 1
                        },
                        {
                            "Type": "Item",
                            "Value": "(O)248",
                            "Amount": 1
                        },
                        {
                            "Type": "ContextTag",
                            "Value": "scallop_item",
                            "Amount": 1,
                            "OverrideText": "Any scallop",
                            "OverrideTexturePath": "Mods/{{ModId}}/TileSheets/Objects",
                            "OverrideTextureRect": {
                                "X": 32,
                                "Y": 48,
                                "Width": 16,
                                "Height": 16
                            }
                        }
                    ]
                },
